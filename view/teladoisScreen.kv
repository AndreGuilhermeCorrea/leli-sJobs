<TelaDoisScreen>:
  
    MDCard:
        size_hint: None, None
        size: 280, 500
        pos_hint: {"center_x": 0.5, "center_y": 0.5}
        orientation: "vertical"

        
        MDFloatLayout:
            orientation:'vertical'
            
            Image:
                source: "pictures/Lelis tela 2.png"
                pos_hint: {"center_x": 0.5, "center_y": 0.5}
                size: 280, 500


            MDIconButton: 
                icon_size: "34sp"
                pos_hint: {"center_x": .85, "center_y": .9}
                icon:"pictures/logout-custom (1).png"
                on_release: root.sair() 
                                

            MDFloatLayout:
                size_hint: .60, .25
                focus_behavior: True
                
                pos_hint: {"center_x": .30, "center_y": .50}
                
                MDIconButton: 
                    pos_hint: {"center_x": .5, "center_y": .6}
                    icon: "pictures/account-hard-hat-custom (1).png" 
                    icon_size: "64sp" 
                    on_release: root.tela_servicos()        
            
                MDLabel:
                    text:"Freelancer"
                    theme_text_color: "Custom"
                    text_color: 74/255, 144/255, 226/255, 1
                    font_size: "22sp"
                    halign: "center"
                    adaptive_size: True
                    bold: True
                    pos_hint: {"center_x": .5, "center_y": .1}            
        

            MDFloatLayout:
                size_hint: .60, .25
                focus_behavior: True
                pos_hint: {"center_x": .7, "center_y": .50}

                MDIconButton: 

                    pos_hint: {"center_x": .5, "center_y": .6}
                    icon: "pictures/account-circle-custom.png"
                    icon_size: "64sp"
                    on_release: root.tela_servicos()
                
                MDLabel:
                    text:"Usu√°rio"
                    theme_text_color: "Custom"
                    text_color: 74/255, 144/255, 226/255, 1
                    font_size: "22sp"
                    halign: "center"
                    adaptive_size: True
                    bold: True
                    pos_hint: {"center_x": .5, "center_y": .1}  
                    
                    
                    
            MDFloatLayout:
                size_hint: .60, .25
                background_color: 0, 0, 0, 0
                pos_hint: {"center_x": .5, "center_y": .3}                        
                
                MDIconButton: 
                    icon_size: "40sp"
                    pos_hint: {"center_x": .5, "center_y": .1}
                    icon:"pictures/pan-left-custom.png"
                    on_release: root.tela_login()                 
                        